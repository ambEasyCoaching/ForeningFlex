<h1>Register</h1>

<%= form_for @user, :url => account_path, :html => { :class => "form-horizontal" } do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  
  <div class="control-group">
    <div class="controls">
      <%= f.submit "Register", :class => "btn" %>
    </div>
  </div>
  
<% end %>
