<h1>Log ind</h1>
<%= form_for @user_session, :url => user_session_path, :method => :post do |f| %>
  <%= f.label :login %><br>
  <%= f.text_field :login %><br>
  <br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <br>
  <%= f.check_box :remember_me %><%= f.label :remember_me %><br>
  <%= f.submit "Log ind", :class => "btn btn-primary" %>
<% end %>